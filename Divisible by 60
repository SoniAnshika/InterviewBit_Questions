int Solution::divisibleBy60(vector<int> &A) {
    int flag=0, t=0;
    int s=0;
    for(int i=0;i<A.size();i++)
    {
        s+=A[i];
        if(A[i]==0)
        {
            flag=1;
        }
        if(A[i]%2==0)
        {
            t++;
        }
       
    }
   
    if((s%3==0) && (flag==1) && (t>=2) || s==0 )
    {
        return 1;
    }
    return 0;
}
